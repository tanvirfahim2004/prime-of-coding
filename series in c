//1*3 + 2*5 +3*7........+n1*n2
#include <stdio.h>

int main(){
    int a1=1,a2=3,sum=0,n1,n2;
    printf("Enter n1 and n2 :");
    scanf("%d%d",&n1,&n2);
    printf("1*3 + 2*5 + 3*7........%d*%d",n1,n2);

    while(a1<=n1 && a2<=n2)
    {
    sum = sum+(a1*a2);
    a1=a1+1;
    a2=a2+2;
    }
    printf("=%d\n",sum);
    return 0;
}




